import java.util.Scanner;

public class exercisesEleventoTwenty
{
    public static void main(String[] args){
        Scanner teclado = new Scanner(System.in);
        System.out.println("Write a money value:");
        Double x = teclado.nextDouble();
        if (x > 100000){
            Double a = x*0.3;
            System.out.println("Tax value is " + a);
        }
        if (x <= 100000 && x >= 50000){
            Double b = x*0.2;
            System.out.println("Tax value is " + b);
        }
        if (x < 50000){
            Double c = x*0.1;
            System.out.println("Tax value is " + c);
        }
    }
}
