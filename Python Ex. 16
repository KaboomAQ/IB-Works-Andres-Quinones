print("Write an integer:")
x=int(input())
print("Write another integer:")
y=int(input())
if x > y:
  print(x," > ",y)
elif x == y:
  print(x," = ",y)
elif x < y:
  print(x," < ",y)
if x < 0:
  print(x," is negative")
if x == 0:
  print(x," is zero")
if x > 0:
  print(x," is positive")
if y < 0:
  print(y," is negative")
if y == 0:
  print(y," is zero")
if y > 0:
  print(y," is positive")
